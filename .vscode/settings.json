{
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "editor.tabSize": 2,
  "json.schemas": [
    {
      "fileMatch": [
        "appsscript.json"
      ],
      "url": "http://json.schemastore.org/appsscript"
    },
    {
      "fileMatch": [
        ".clasp.json"
      ],
      "url": "http://json.schemastore.org/clasp"
    }
  ],
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "search.exclude": {
    "**/dist": true
  }
}