{
  "compilerOptions": {
    "jsx": "preserve",
    "jsxFactory": "formulaireBleuJSXFactory",
    "jsxFragmentFactory": "formulaireBleuJSXFragmentFactory",
    "lib": [
      "DOM",
      "ES2020"
    ],
    "module": "ES6",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "dist",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "target": "ES6"
  },
  "include": [
    "src/**/*"
  ]
}